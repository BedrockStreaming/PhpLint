{
    "name": "m6web/php-lint",
    "description": "Simple command to run php lint",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "M6Web",
            "email": "opensource@m6web.fr",
            "homepage": "http://tech.m6web.fr/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/process": "^2.8||^3.0||^4.0",
        "symfony/finder": "^2.8||^3.0||^4.0",
        "symfony/console": "^2.8||^3.0||^4.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.3",
        "atoum/stubs": "^2.5",
        "m6web/coke"                       : "~2.2",
        "m6web/symfony2-coding-standard"   : "~3.3"
    },
    "autoload": {
        "psr-4": {
            "M6Web\\PhpLint\\": ""
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "bin": [
        "bin/phplint"
    ]
}
